//Write a program to input a character print whether it is vowel or consonant.
#include<stdio.h>
int main()
{
	char ch;
	int vowel,consonant;
	printf("Enter an alphabet:");
	scanf("%d",&ch);
	switch(ch)
	{
		case 'a':
		case 'e':
		case 'i':
		case 'o':
		case 'u':
		case 'A':
		case 'E':
		case 'I':
		case 'O':
		case 'U':
		printf("Vowel");
		break;
		default:
			printf("Consonant");
	}
	return 0;
}
