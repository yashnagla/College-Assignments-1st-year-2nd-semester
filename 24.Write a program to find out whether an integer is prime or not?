//Write a program to find out whether an integer is prime or not.
#include<stdio.h> 
int main() 
{
    int n,i,c=0;
    printf("Enter any number\n");
    scanf("%d",&n);
  
    for(i=1;i<= n;i++)
    {
        if (n%i==0) 
        {
            c++;
        }
    }
    if(c==2)
    {
  	    printf("\n%d is a Prime number",n);
    }
    else 
    {
 	    printf("\n%d is not a Prime number",n);
    }
}
